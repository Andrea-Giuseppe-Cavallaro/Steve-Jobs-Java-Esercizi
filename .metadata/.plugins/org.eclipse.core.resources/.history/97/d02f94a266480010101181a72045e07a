package _09_Utente_e_Permessi;

import java.util.ArrayList;

public class Cliente extends Utente{

	Cliente(String username, String email) {
		super(username, email);
	}

	@Override
	public ArrayList<String> getPermessi() {
		ArrayList<String> permessi = null;
		permessi.add("READ");
		return permessi;
	}

}
